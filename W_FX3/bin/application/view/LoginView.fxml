<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="250.0" prefWidth="350.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="application.controller.LoginController">
   <children>
      <TextField fx:id="tf_login" layoutX="87.0" layoutY="72.0" promptText="Login" />
      <TextField fx:id="tf_pasword" layoutX="87.0" layoutY="151.0" />
      <PasswordField fx:id="pf_password" layoutX="87.0" layoutY="151.0" promptText="Password" />
      <Label layoutX="148.0" layoutY="38.0" text="Login" textAlignment="CENTER" />
      <Label layoutX="148.0" layoutY="117.0" text="Password" textAlignment="CENTER" />
      <Button fx:id="btn_login" layoutX="140.0" layoutY="198.0" mnemonicParsing="false" onMouseClicked="#buttonLogin" text="Login" textAlignment="CENTER" />
      <Button fx:id="btn_show" layoutX="236.0" layoutY="151.0" mnemonicParsing="false" onMouseClicked="#showPass" text="show" />
   </children>
</AnchorPane>
